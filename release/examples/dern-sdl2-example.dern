(require 'dern_sdl2)

(sdl2-Init 'EVERYTHING)
(define window  as (sdl2-CreateWindow [test] 0 0 100 100) [window])
(define surface as (sdl2-GetWindowSurface window) [surface])
(sdl2-FillRect surface nil (sdl2-MapRGB (sdl2-GetPixelFormat surface) 255 255 0))
(sdl2-UpdateWindowSurface window)
(sdl2-Delay 3.0)
(sdl2-Quit)

